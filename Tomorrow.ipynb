   "source": [
    "<h4>Creating Training Data</h4>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "89c0cb84-6c06-4937-8a9d-ccb0a1533d63",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
